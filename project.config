[access]
	inheritFrom = All-Projects
[project]
	description = Lighting projects
[access "refs/for/refs/heads/*"]
	push = group LightingFramework
[access "refs/heads/*"]
	abandon = group LightingFramework
	create = group LightingFramework
	forgeAuthor = group LightingFramework
	label-Code-Review = -2..+2 group LightingFramework
	submit = group LightingFramework
	editTopicName = group LightingFramework
[access "refs/for/*"]
	pushMerge = group LightingFramework
[access "refs/meta/config"]
	read = group LightingFramework
[access "refs/tags/*"]
	pushTag = group LightingFramework
	pushSignedTag = group LightingFramework
[access "refs/for/refs/heads/feature/*"]
	pushMerge = group LightingFramework
	pushMerge = group Non-Interactive Users
[access "refs/*"]
	read = group GitHub Replication
