[access]
	inheritFrom = All-Projects
[project]
	description = Lighting projects
[access "refs/for/refs/heads/*"]
	push = group Lighting
	push = group ldap/lighting_service_framework
[access "refs/heads/*"]
	abandon = group Lighting
	abandon = group ldap/lighting_service_framework
	create = group Lighting
	create = group ldap/lighting_service_framework
	forgeAuthor = group Lighting
	forgeAuthor = group ldap/lighting_service_framework
	label-Code-Review = -2..+2 group Lighting
	label-Code-Review = -2..+2 group ldap/lighting_service_framework
	submit = group Lighting
	submit = group ldap/lighting_service_framework
	editTopicName = group Lighting
	editTopicName = group ldap/lighting_service_framework
[access "refs/for/*"]
	pushMerge = group Lighting
	pushMerge = group ldap/lighting_service_framework
[access "refs/meta/config"]
	read = group Lighting
	read = group ldap/lighting_service_framework
[access "refs/tags/*"]
	pushTag = group Lighting
	pushTag = group ldap/lighting_service_framework
	pushSignedTag = group Lighting
	pushSignedTag = group ldap/lighting_service_framework
[access "refs/for/refs/heads/feature/*"]
	pushMerge = group Lighting
	pushMerge = group Non-Interactive Users
	pushMerge = group ldap/lighting_service_framework
[access "refs/*"]
	read = deny group GitHub Replication for lighting
