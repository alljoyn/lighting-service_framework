<busconfig>
  <type>alljoyn</type>
  <listen>unix:abstract=alljoyn</listen>
  <listen>tcp:iface=*,port=9955</listen>
  <listen>udp:iface=*,port=9955</listen>
  <limit name="auth_timeout">20000</limit>
  <limit name="max_incomplete_connections">120</limit>
  <limit name="max_completed_connections">120</limit>
  <limit name="max_untrusted_clients">120</limit>
  <flag name="restrict_untrusted_clients">false</flag>
</busconfig>